<div class="row">
    <div class="resend-confirmation-instructions">
        <div id="" class="static_page">
            <div class="modal-header">
                <h3>
                    Resend confirmation instructions
                </h3>
            </div>
            <form id="" ng-controller="DummyCtrl" name="" autocomplete="" class="ng-scope ng-pristine ng-valid" action="/confirmation" method="POST" enctype="">
                <!-- <input type="hidden" name="csrfmiddlewaretoken" value="k1XVXoPqPeWwmRIPvSXtEvuvnPV3rlCb"> -->
                <div class="modal-body clearfix">
                    <div class="form-group ">
                        <label class="control-label  " for="id_email">E-mail</label>
                        <div class=" ">
                            <input class="form-control" id="id_email" name="email" type="email">
                        </div>
                        <% if (error === 'empty_field') { %>
                            <span class="help-block alert alert-danger ">This field is required.</span>
                        <% } %>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Resend confirmation instructions</button>
                    <div class="actions">
                        <a href="/sign_up/">
                            Sign up
                        </a>
                        <a href="/password/request/">
                            Forgot password
                        </a>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>