<div id="content_body">
    <div id="detailOrganization">
        <header>
            <h3 class="name">
                <%= organization.name %>
                <a href="/idm/organizations/<%= organization.id %>/edit/" class="small">
                    <i class="fa fa-edit"></i>
                    <span>edit</span>
                </a>
            </h3>
            <div class="logo">
                <img alt="organization logo" src="<%= organization.image %>">
            </div>
        </header>
        <div class="panel panel-default">
            <div class="panel-body">
                <div class="description">
                    <h5>
                        Description
                    </h5>
                    <div class="info expander">
                        <p>
                        <%  var lines = organization.description.split(/\r\n|\r|\n/)%>
                        <% for (var i = 0; i < lines.length; i++) { -%>
                            <% if (lines[i].replace(/^\s+/, '').replace(/\s+$/, '') !== '') { -%>
                                <%= lines[i] %>
                                <br>
                            <% } -%>
                        <% } -%>
                        </p>
                    </div>
                </div> 
            </div>
        </div>
         <div class="panel panel-default datatable" id="auth_users" data-pagination-url="/filters/users" data-application_id="<%= organization.id %>" data-pagination-pages="">
            <div class="panel-heading">
                <span class="title">Members</span>
                <div class="table_actions clearfix">
                    <div class="filter table_search client">
                        <input class="form-control" value="" type="text" name="members__filter__q" placeholder="Filter">
                    </div>
                    <div id="spinner_members" class="filtering-spinner-inline" style="display: none;"><i class="fa fa-circle-o-notch fa-spin"></i></div>
                    <!-- PONER AQUI EL MANAGE SI EL USUARIO ES OWNER -->
                        <div class="btn-group"> 
                            <button title="Manage" class="btn btn-default btn-sm ajax-modal" data-toggle="modal" data-target="#manage_member" id="members__action_manage_members">
                                <i class="fa fa-cogs"></i> 
                                <span class="action">
                                    Manage
                                </span>
                            </button>
                        </div>
                </div>
            </div>
            <div class="panel-body datatable-content">
                <p class="alert alert-info empty" style="display: block;">This organization does not have any member.</p>
