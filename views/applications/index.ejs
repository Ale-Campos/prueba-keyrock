<div id="content_body">
	<div class="page-header">
		<h2>
			Applications
		</h2>
	</div>
	<div id="myApplications-table">
		<div class="panel panel-default tab-group">
			<div id="header_panel_tabs" class="panel-heading">
				<div class="tab_group_actions clearfix">
					<div class="btn-group"> 
						<a href="/idm/applications/new/" title="Register" class="btn btn-primary">
							Register
						</a>
					</div>
				</div>
				<ul id="panel_tabs" class="nav nav-tabs ajax-tabs" data-sticky-tabs="sticky">
					<li class="active">
						<a href="?tab=panel_tabs__providing_tab" data-toggle="tab" data-target="#panel_tabs__providing_tab" data-loaded="true">Providing</a>
					</li>
					<li class="">
						<a href="?tab=panel_tabs__purchased_tab" data-toggle="tab" data-target="#panel_tabs__purchased_tab" data-loaded="true">Purchased</a>
					</li>
					<li class="">
						<a href="?tab=panel_tabs__authorized_tab" data-toggle="tab" data-target="#panel_tabs__authorized_tab" data-loaded="true">Authorized</a>
					</li>
				</ul>
				<div id="spinner_panel_tabs" class="tab-spinner-inline" style="display: none;"><i class="fa fa-circle-o-notch fa-spin"></i>
				</div>
			</div>
			<div id="body_panel_tabs" class="panel-body tab-content">
				<div id="panel_tabs__providing_tab" class="tab-pane active">
					<div class=" datatable" id="providing_table" data-pagination-url="/filters/applications" data-application_role="106" data-pagination-pages="">
						<div class="list-group" id="providing_table_content">
							<% for (var i = 0; i < applications.length; i++) { -%>
								<div class="list-group-item">
									<a class="item" href="/idm/applications/<%= applications[i].id%>/">
										<div class="avatar filter_field">
											<img src="<%= applications[i].image %>">
										</div>	
										<div class="name filter_field"><%= applications[i].name %></div>
										<div class="url filter_field"><%= applications[i].url %></div>
									</a>
								</div>
							<% } -%>
						</div>
						<% if (typeof applications == "undefined") { -%>
							<p class="alert alert-info empty" style="display: block;">You are not provider of any application.</p>
						<% } -%>	
						<div class="panel-footer">
							<nav id="providing_table_pagination_container">
								<ul class="pagination bootpag">
									<li data-lp="1" class="first disabled">
										<a href="javascript:void(0);">First</a>
									</li>
									<li data-lp="1" class="prev disabled">
										<a href="javascript:void(0);">«</a>
									</li>
									<li data-lp="1" class="active">
										<a href="javascript:void(0);">1</a>
									</li>
									<li data-lp="1" class="next disabled">
										<a href="javascript:void(0);">»</a>
									</li>
									<li data-lp="1" class="last disabled">
										<a href="javascript:void(0);">Last</a>
									</li>
								</ul>
							</nav>
						</div>
					</div>
				</div>
				<div id="panel_tabs__purchased_tab" class="tab-pane">
					<div class=" datatable" id="purchased_table" data-pagination-url="/filters/applications" data-application_role="191" data-pagination-pages="">
						<div class="list-group" id="purchased_table_content"></div>
							<% for (var i = 0; i < applications.length; i++) { -%>
								<div class="list-group-item">
									<a class="item" href="/idm/applications/<%= applications[i].id%>/">
										<div class="avatar filter_field">
											<img src="<%= applications[i].image %>">
										</div>
										<div class="name filter_field"><%= applications[i].name %></div>
										<div class="url filter_field"><%= applications[i].url %></div>
									</a>
								</div>
							<% } -%>
						<% if (typeof applications == "undefined") { -%>
							<p class="alert alert-info empty" style="display: block;">You are not provider of any application.</p>
						<% } -%>
						<div class="panel-footer">
							<nav id="purchased_table_pagination_container">
								<ul class="pagination bootpag">
									<li data-lp="1" class="first disabled">
										<a href="javascript:void(0);">First</a>
									</li>
									<li data-lp="1" class="prev disabled">
										<a href="javascript:void(0);">«</a>
									</li>
									<li data-lp="1" class="active">
										<a href="javascript:void(0);">1</a>
									</li>
									<li data-lp="1" class="next disabled">
										<a href="javascript:void(0);">»</a>
									</li>
									<li data-lp="1" class="last disabled">
										<a href="javascript:void(0);">Last</a>
									</li>
								</ul>
							</nav>
						</div>
					</div>
				</div>
				<div id="panel_tabs__authorized_tab" class="tab-pane">
					<div class=" datatable" id="authorized_table" data-pagination-url="/filters/applications" data-application_role="OTHER" data-pagination-pages="">
						<div class="list-group" id="authorized_table_content">
							<% for (var i = 0; i < applications.length; i++) { -%>
								<div class="list-group-item">
									<a class="item" href="/idm/applications/<%= applications[i].id%>/">
										<div class="avatar filter_field">
											<img src="<%= applications[i].image %>">
										</div>
										<div class="name filter_field"><%= applications[i].name %></div>
										<div class="url filter_field"><%= applications[i].url %></div>
									</a>
								</div>
							<% } -%>
						</div>
						<% if (typeof applications == "undefined") { -%>
							<p class="alert alert-info empty" style="display: block;">You are not provider of any application.</p>
						<% } -%>
						<div class="panel-footer">
							<nav id="authorized_table_pagination_container">
								<ul class="pagination bootpag">
									<li data-lp="1" class="first disabled">
										<a href="javascript:void(0);">First</a>
									</li>
									<li data-lp="1" class="prev disabled">
										<a href="javascript:void(0);">«</a>
									</li>
									<li data-lp="1" class="active">
										<a href="javascript:void(0);">1</a>
									</li>
									<li data-lp="1" class="next disabled">
										<a href="javascript:void(0);">»</a>
									</li>
									<li data-lp="1" class="last disabled">
										<a href="javascript:void(0);">Last</a>
									</li>
								</ul>
							</nav>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>